  {
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Electron: Main",
        "protocol": "inspector",
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd",
        "runtimeArgs": [
          "--remote-debugging-port=9223",
          "."
        ],
        "windows": {
          "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
        },
        "env": {
          "NODE_ENV": "dev"
        },
      }
    ]
  }
